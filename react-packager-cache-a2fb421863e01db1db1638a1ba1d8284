rn 3 * (2 * (t - 1) * t + v * v) * x1 + 3 * (- t * t * t + 2 * v * t) * x2;\n  };\n\n  return function(t){\n\n    var x = t, t0, t1, t2, x2, d2, i;\n\n    // First try a few iterations of Newton's method -- normally very fast.\n    for (t2 = x, i = 0; i < 8; i++){\n      x2 = curveX(t2) - x;\n      if (Math.abs(x2) < epsilon) return curveY(t2);\n      d2 = derivativeCurveX(t2);\n      if (Math.abs(d2) < 1e-6) break;\n      t2 = t2 - x2 / d2;\n    }\n\n    t0 = 0, t1 = 1, t2 = x;\n\n    if (t2 < t0) return curveY(t0);\n    if (t2 > t1) return curveY(t1);\n\n    // Fallback to the bisection method for reliability.\n    while (t0 < t1){\n      x2 = curveX(t2);\n      if (Math.abs(x2 - x) < epsilon) return curveY(t2);\n      if (x > x2) t0 = t2;\n      else t1 = t2;\n      t2 = (t1 - t0) * .5 + t0;\n    }\n\n    // Failure\n    return curveY(t2);\n\n  };\n\n};\n","sourcePath":"/Users/ranjith/Development/Learning/React_Blogs/FancyLogin/fancyLogin/node_modules/react-native/Libraries/Animated/src/bezier.js"},"haste":true}},"/Users/ranjith/Development/Learning/React_Blogs/FancyLogin/fancyLogin/node_modules/react-native/Libraries/Animated/src/polyfills/InteractionManager.js":{"metadata":{"mtime":1443105172000},"data":{"haste":false}},"/Users/ranjith/Development/Learning/React_Blogs/FancyLogin/fancyLogin/node_modules/react-native/Libraries/Animated/src/polyfills/Set.js":{"metadata":{"mtime":1443105172000},"data":{"haste":false}},"/Users/ranjith/Development/Learning/React_Blogs/FancyLogin/fancyLogin/node_modules/react-native/Libraries/Animated/src/polyfills/flattenStyle.js":{"metadata":{"mtime":1443105172000},"data":{"haste":false}},"/Users/ranjith/Development/Learning/React_Blogs/FancyLogin/fancyLogin/node_modules/react-native/Libraries/CustomComponents/ListView/ListView.js":{"metadata":{"mtime":1444322992000},"data":{"name":"ListView","dependencies":["ListViewDataSource","React","NativeModules","NativeModules","ScrollView","ScrollResponder","StaticRenderer","react-timer-mixi