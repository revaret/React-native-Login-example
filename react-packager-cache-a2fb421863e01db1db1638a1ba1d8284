the `aps` object\n   */\n  getSound(): ?string {\n    return this._sound;\n  }\n\n  /**\n   * Gets the notification's main message from the `aps` object\n   */\n  getAlert(): ?string | ?Object {\n    return this._alert;\n  }\n\n  /**\n   * Gets the badge count number from the `aps` object\n   */\n  getBadgeCount(): ?number {\n    return this._badgeCount;\n  }\n\n  /**\n   * Gets the data object on the notif\n   */\n  getData(): ?Object {\n    return this._data;\n  }\n}\n\nmodule.exports = PushNotificationIOS;\n","sourcePath":"/Users/ranjith/Development/Learning/React_Blogs/FancyLogin/fancyLogin/node_modules/react-native/Libraries/PushNotificationIOS/PushNotificationIOS.js"},"haste":true}},"/Users/ranjith/Development/Learning/React_Blogs/FancyLogin/fancyLogin/node_modules/react-native/Libraries/QuickPerformanceLogger/QuickPerformanceLogger.js":{"metadata":{"mtime":1444322992000},"data":{"name":"QuickPerformanceLogger","haste":true}},"/Users/ranjith/Development/Learning/React_Blogs/FancyLogin/fancyLogin/node_modules/react-native/Libraries/RKBackendNode/queryLayoutByID.js":{"metadata":{"mtime":1442235473000},"data":{"name":"queryLayoutByID","dependencies":["ReactNativeTagHandles","NativeModules"],"transformedSource":{"code":"'use strict';\n\n\n\n\n\n\n\n\n\n\n\n\nvar ReactNativeTagHandles=require('ReactNativeTagHandles');\nvar RCTUIManager=require('NativeModules').UIManager;\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nvar queryLayoutByID=function(\nrootNodeID,\nonError,\nonSuccess)\n{\n\nRCTUIManager.measure(\nReactNativeTagHandles.rootNodeIDToTag[rootNodeID],\nonSuccess);};\n\n\n\nmodule.exports = queryLayoutByID;","sourceCode":"/**\n * Copyright (c) 2015-present, Facebook, Inc.\n * All rights reserved.\n *\n * This source code is licensed under the BSD-style license found in the\n * LICENSE file in the root directory of this source tree. An additional grant\n * of patent rights can be found in the PATENTS file in the same directory.\n *\n * @prov